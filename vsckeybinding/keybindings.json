// 将键绑定放入此文件中以覆盖默认值
[
    { "key": "ctrl+e",                   "command": "cursorEnd",
                                     "when": "editorTextFocus" },
    { "key": "",          "command": "workbench.view.explorer" },
    { "key": "ctrl+shift+e",                "command": "workbench.action.quickOpen" },
    { "key": "ctrl+n",                  "command": "cursorDown",
                                     "when": "editorTextFocus" },
    { "key": "ctrl+f",                 "command": "cursorRight",
                                     "when": "editorTextFocus" },
    { "key": "ctrl+b",                  "command": "cursorLeft",
                                     "when": "editorTextFocus" },
    { "key": "ctrl+p",                    "command": "cursorUp",
                                     "when": "editorTextFocus" },
    { "key": "",                "command": "workbench.action.quickOpen" },
    { "key": "",                "command": "workbench.action.files.newUntitledFile" },
    { "key": "ctrl+shift+f",                "command": "actions.find" },
    { "key": "",          "command": "workbench.view.search" },
    { "key": "",                "command": "workbench.action.toggleSidebarVisibility" }
]